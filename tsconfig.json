{
	"compilerOptions": {
		"lib": ["es2018", "es2019"],
		"target": "ES2018",
		"module": "commonjs",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"outDir": "dist"
	}
}
